<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.net.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="acTestMenu" styleClass="sub-main-frame"
      xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="lk.ijse.mountCalvary.controller.basic.TestMenu_controller">
    <children>
        <Region prefHeight="10.0" prefWidth="1280.0"/>
        <JFXButton fx:id="btBack" onAction="#btBack_onAction" prefHeight="45.0" prefWidth="69.0" styleClass="back"
                   text="Back">
            <graphic>
                <FontAwesomeIconView glyphName="ARROW_LEFT" size="25"/>
            </graphic>
            <VBox.margin>
                <Insets left="10.0"/>
            </VBox.margin>
        </JFXButton>
        <VBox alignment="CENTER" prefHeight="67.0" styleClass="menu-button-pane" VBox.vgrow="ALWAYS">
            <children>
                <JFXButton fx:id="btTestProfile" onAction="#btTestProfile_onAction" prefHeight="65.0" prefWidth="554.0"
                           style="-fx-background-color: #2196F3;" styleClass="menu-button" text="Test Profile"
                           textFill="WHITE">
                    <font>
                        <Font size="27.0"/>
                    </font>
                </JFXButton>
                <JFXButton fx:id="btNewTerm" onAction="#btNewTerm_onAction" prefHeight="65.0" prefWidth="554.0"
                           style="-fx-background-color: #2196F3;" styleClass="menu-button" text="New Term"
                           textFill="WHITE">
                    <font>
                        <Font size="27.0"/>
                    </font>
                </JFXButton>
                <JFXButton fx:id="btPhysicalEvaluationTest" onAction="#btPhysicalEvaluationTest_onAction"
                           prefHeight="65.0" prefWidth="554.0" style="-fx-background-color: #2196F3;"
                           styleClass="menu-button" text="Physical Evaluation Test" textFill="WHITE">
                    <font>
                        <Font size="27.0"/>
                    </font>
                </JFXButton>
            </children>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@../../css/frame.css"/>
        <URL value="@../../css/button.css"/>
        <URL value="@../../css/combo-box.css"/>
        <URL value="@../../css/table-view.css"/>
        <URL value="@../../css/text-and-password-field.css"/>
        <URL value="@../../css/date-picker.css"/>
        <URL value="@../../css/radio-button.css"/>
        <URL value="@../../css/text-area.css"/>
        <URL value="@../../css/label.css"/>
    </stylesheets>
</VBox>
