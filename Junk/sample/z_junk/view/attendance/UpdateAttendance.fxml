<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="acUpdateAttendance" prefHeight="785.0" prefWidth="1235.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="sample.z_junk.controller.attendance.UpdateAttendance_controller">
   <children>
      <Label layoutX="85.0" layoutY="24.0" prefHeight="42.0" prefWidth="209.0" text="Update attendance">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <JFXComboBox fx:id="cboxActivity" editable="true" labelFloat="true" layoutX="85.0" layoutY="88.0" onAction="#cboxActivity_onAction" prefHeight="25.0" prefWidth="504.0" promptText="Activity" />
      <JFXComboBox fx:id="cboxTeacherInCharge" labelFloat="true" layoutX="85.0" layoutY="140.0" onAction="#cboxTeacherInCharge_onAction" prefHeight="25.0" prefWidth="504.0" promptText="Teacher in charge" />
      <JFXDatePicker fx:id="dpDate" layoutX="998.0" layoutY="88.0" onAction="#dpDate_onAction" promptText="Date" showWeekNumbers="true" />
      <TableView fx:id="tblStudentList" layoutX="81.0" layoutY="184.0" prefHeight="199.0" prefWidth="1074.0">
        <columns>
          <TableColumn fx:id="colStudentList" prefWidth="75.0" text="Student list" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TableView fx:id="tblAttendance" layoutX="81.0" layoutY="454.0" prefHeight="231.0" prefWidth="1074.0">
         <columns>
            <TableColumn fx:id="colStudent" prefWidth="75.0" text="Student " />
            <TableColumn fx:id="colActivity" prefWidth="75.0" text="Activity" />
            <TableColumn fx:id="colTeacherInCharge" prefWidth="75.0" text="Teacher in charge" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton fx:id="btCancel" layoutX="897.0" layoutY="703.0" onAction="#btCancel_onAction" prefHeight="33.0" prefWidth="124.0" style="-fx-background-color: #2196F3;" text="Cancel" textFill="WHITE" />
      <JFXButton fx:id="btUpdate" layoutX="1031.0" layoutY="703.0" onAction="#btUpdate_onAction" prefHeight="33.0" prefWidth="124.0" style="-fx-background-color: #2196F3;" text="Update" textFill="WHITE" />
      <JFXButton fx:id="btRemove" layoutX="1031.0" layoutY="429.0" onAction="#btRemove_onAction" prefHeight="18.0" prefWidth="124.0" text="Remove" textFill="#2196f3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btAddAll" layoutX="81.0" layoutY="396.0" onAction="#btAddAll_onAction" prefHeight="33.0" prefWidth="124.0" style="-fx-background-color: #2196F3;" text="Add all" textFill="WHITE" />
   </children>
</AnchorPane>
