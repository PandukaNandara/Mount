<VBox fx:id="competitionForStudent" styleClass="profile-frame" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.mountCalvary.controller.student.profile.CompetitionForStudentController">
   <children>
      <HBox prefHeight="23.0" prefWidth="1340.0" styleClass="node-pane" VBox.vgrow="NEVER">
         <children>
              <Label maxWidth="1.7976931348623157E308" prefHeight="36.0" prefWidth="309.0" styleClass="title" text="Competition and achievement" HBox.hgrow="ALWAYS">
                  <font>
                      <Font name="System Bold" size="18.0" />
                  </font>
              </Label>
              <JFXComboBox fx:id="cboxCompetition" editable="true" labelFloat="true" onAction="#cboxCompetition_onAction" prefHeight="27.0" prefWidth="56.0" promptText="Competition" visible="false" />
              <JFXComboBox fx:id="cboxActivity" labelFloat="true" onAction="#cboxActivity_onAction" prefHeight="29.0" prefWidth="52.0" promptText="Activity" visible="false" />
            <JFXButton fx:id="btPrint" alignment="CENTER" buttonType="RAISED" onAction="#btPrint_onAction" prefHeight="45.0" prefWidth="108.0" ripplerFill="WHITE" style="-fx-background-color: #2196f3;" text="Print" textAlignment="CENTER" textFill="WHITE">
               <graphic>
                  <Icons525View fill="WHITE" glyphName="PRINTER" size="25" wrappingWidth="37.0" />
               </graphic>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
         </children>
      </HBox>
        <TableView fx:id="tblParticipation" editable="true" prefHeight="613.0" tableMenuButtonVisible="true" prefWidth="1340.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="colCompetition" prefWidth="210.4212646484375" text="Competition" />
                <TableColumn fx:id="colDate" prefWidth="113.00006103515625" text="Date" />
                <TableColumn fx:id="colActivity" prefWidth="166.0" text="Activity" />
                <TableColumn fx:id="colEvent" prefWidth="157.3055419921875" text="Event" />
                <TableColumn fx:id="colResult" prefWidth="158.1666259765625" text="Result" />
                <TableColumn fx:id="colPerformance" prefWidth="144.0" text="Performance" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
   </children>
    <stylesheets>
        <URL value="@../../../css/frame.css" />
