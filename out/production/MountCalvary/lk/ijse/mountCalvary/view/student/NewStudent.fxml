<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="acNewStudent" prefHeight="785.0" prefWidth="1235.0" style="-fx-background-color: white;" styleClass="main-frame" stylesheets="@../../css/size.css" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="lk.ijse.mountCalvary.controller.student.NewStudent_controller">
   <children>
      <JFXTextField fx:id="txtStudentName" labelFloat="true" layoutX="253.0" layoutY="92.0" onAction="#txtStudentName" prefHeight="25.0" prefWidth="1086.0" promptText="Student name" />
      <Label layoutX="66.0" layoutY="36.0" prefHeight="36.0" prefWidth="309.0" text="Personal Details">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <JFXRadioButton fx:id="rbMale" layoutX="329.0" layoutY="141.0" prefHeight="17.0" prefWidth="131.0" selected="true" stylesheets="@../../css/size.css" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </JFXRadioButton>
      <JFXRadioButton fx:id="rbFemale" layoutX="459.0" layoutY="141.0" onAction="#rbFemale" text="Female" toggleGroup="$gender" />
      <JFXComboBox fx:id="cboxClass" labelFloat="true" layoutX="329.0" layoutY="189.0" onAction="#cboxClass_onAction" prefHeight="25.0" prefWidth="262.0" promptText="Class" />
      <JFXComboBox fx:id="cboxGrade" labelFloat="true" layoutX="68.0" layoutY="189.0" onAction="#cboxGrade_onAction" prefHeight="25.0" prefWidth="228.0" promptText="Grade" />
      <JFXDatePicker fx:id="dtDOB" layoutX="634.0" layoutY="186.0" onAction="#dtDOB_onAction" prefHeight="25.0" prefWidth="708.0" promptText="Birth day" showWeekNumbers="true" />
      <JFXTextField fx:id="txtMotherName" labelFloat="true" layoutX="67.0" layoutY="248.0" onAction="#txtMotherName_onAction" prefHeight="25.0" prefWidth="585.0" promptText="Mother's name" />
      <JFXTextField fx:id="txtFatherName" labelFloat="true" layoutX="692.0" layoutY="248.0" onAction="#txtFatherName_onAction" prefHeight="25.0" prefWidth="647.0" promptText="Father's name" />
      <JFXTextField fx:id="txtStudentID" labelFloat="true" layoutX="68.0" layoutY="92.0" onAction="#txtStudentID_onAction" prefHeight="19.0" prefWidth="161.0" promptText="Student ID" />
      <Label layoutX="253.0" layoutY="141.0" prefHeight="17.0" prefWidth="105.0" text="Gender" />
      <JFXTextArea fx:id="txtaDesc" labelFloat="true" layoutX="69.0" layoutY="512.0" onDragDetected="#txtaDesc_onAction" prefHeight="50.0" prefWidth="1272.0" promptText="About this student" />
      <TableView fx:id="tblTelNo" editable="true" fixedCellSize="0.0" layoutX="64.0" layoutY="408.0" prefHeight="81.0" prefWidth="1282.0">
         <columns>
            <TableColumn fx:id="colTelNo_tblTelNo" prefWidth="820.0" text="Telephone Number" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton fx:id="btRemove_tblTelNo" layoutX="1209.0" layoutY="374.0" onAction="#btRemove_tblTelNo_onAction" prefHeight="12.0" prefWidth="124.0" text="Remove" textFill="#2196f3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btAddTelNo" layoutX="620.0" layoutY="358.0" onAction="#btAddTelNo_onAction" prefHeight="33.0" prefWidth="124.0" style="-fx-background-color: #2196F3;" text="Add" textFill="WHITE" />
      <JFXTextField fx:id="txtTelNo" labelFloat="true" layoutX="66.0" layoutY="365.0" onAction="#txtTelNo_onAction" prefHeight="25.0" prefWidth="502.0" promptText="Telephone number" />
      <JFXTextField fx:id="txtAddress" labelFloat="true" layoutX="67.0" layoutY="310.0" onAction="#txtAddress_onAction" prefHeight="25.0" prefWidth="1269.0" promptText="Address" />
      <TableView fx:id="tblActivity" layoutX="64.0" layoutY="680.0" prefHeight="104.0" prefWidth="1284.0">
         <columns>
            <TableColumn fx:id="colActivity" prefWidth="75.0" text="Activity" />
            <TableColumn fx:id="colJoinedDate" prefWidth="75.0" text="Joined date" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton fx:id="btRemove_tblActivity" layoutX="1222.0" layoutY="650.0" onAction="#btRemove_tblActivity_onAction" prefHeight="19.0" prefWidth="124.0" text="Remove" textFill="#2196f3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXComboBox fx:id="cboxActivityName" labelFloat="true" layoutX="68.0" layoutY="618.0" onAction="#cboxActivityName_onAction" prefHeight="25.0" prefWidth="531.0" promptText="Activity name" />
      <JFXDatePicker fx:id="dtJoinedDate" layoutX="622.0" layoutY="617.0" onAction="#dtJoinedDate_onAction" prefHeight="25.0" prefWidth="558.0" promptText="Joined date" />
      <JFXButton fx:id="btAddActivity" layoutX="1198.0" layoutY="613.0" onAction="#btAddActivity_onAction" prefHeight="33.0" prefWidth="148.0" style="-fx-background-color: #2196F3;" text="Add" textFill="WHITE" />
      <JFXButton fx:id="btFinish_activityForStudent" layoutX="1220.0" layoutY="813.0" onAction="#btFinish_activityForStudent_onAction" prefHeight="33.0" prefWidth="124.0" style="-fx-background-color: #2196F3;" text="Finish" textFill="WHITE" />
      <JFXButton fx:id="btCancel" layoutX="1076.0" layoutY="817.0" onAction="#btCancel_onAction" prefHeight="25.0" prefWidth="124.0" text="Cancel" textFill="#2196f3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <Label layoutX="71.0" layoutY="571.0" prefHeight="36.0" prefWidth="309.0" text="Activity for student">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <JFXComboBox fx:id="cboxHouse" labelFloat="true" layoutX="68.0" layoutY="137.0" onAction="#cboxHouse_onAction" prefHeight="25.0" prefWidth="161.0" promptText="House" />
   </children>
</AnchorPane>
