<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import de.jensd.fx.glyphs.icons525.Icons525View?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="attendanceAndActivityOfStudent" prefHeight="745.0" prefWidth="1022.0" styleClass="profile-frame"
            stylesheets="@../../../css/size.css" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="lk.ijse.mountCalvary.controller.student.profile.AttendanceAndActivityOfStudentController">
    <children>
        <Label layoutX="50.0" layoutY="20.0" prefHeight="36.0" prefWidth="309.0"
               text="Attendance and activity of student">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <TableView fx:id="tblRegistration" layoutX="50.0" layoutY="119.0" prefHeight="108.0" prefWidth="1343.0">
            <columns>
                <TableColumn fx:id="colActivity_tblRegistration" prefWidth="524.0" text="Activity "/>
                <TableColumn fx:id="colJoinedDate_tblRegistration" prefWidth="295.0" text="Joined date"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <TableView fx:id="tblAttendantSheet" layoutX="52.0" layoutY="315.0" prefHeight="429.0" prefWidth="1343.0">
            <columns>
                <TableColumn fx:id="colDate_tblAttendantSheet" prefWidth="295.0" text="Date"/>
                <TableColumn fx:id="colActivity_tblAttendantSheet" prefWidth="524.0" text="Activity "/>
                <TableColumn fx:id="colTeacherInCharge_tblAttendantSheet" prefWidth="295.0" text="Teacher in charge"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <JFXComboBox fx:id="cboxTimeRange" labelFloat="true" layoutX="1093.0" layoutY="277.0"
                     onAction="#cboxTimeRange_onAction" prefHeight="29.0" prefWidth="302.0" promptText="Time range"/>
        <JFXComboBox fx:id="cboxActivity" labelFloat="true" layoutX="53.0" layoutY="277.0"
                     onAction="#cboxActivity_onAction" prefHeight="25.0" prefWidth="295.0" promptText="Activity"/>
        <Label layoutX="50.0" layoutY="228.0" prefHeight="36.0" prefWidth="309.0" text="Attendance ">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="50.0" layoutY="56.0" prefHeight="36.0" prefWidth="309.0" text="Activity">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <JFXButton fx:id="btPrint" alignment="CENTER" buttonType="RAISED" contentDisplay="TOP" layoutX="1310.0"
                   layoutY="23.0" onAction="#btPrint_onAction" prefHeight="77.0" prefWidth="79.0" ripplerFill="WHITE"
                   style="-fx-background-color: #2196f3;" text="Print" textAlignment="CENTER" textFill="WHITE">
            <graphic>
                <Icons525View fill="WHITE" glyphName="PRINTER" size="50" wrappingWidth="49.0"/>
            </graphic>
        </JFXButton>
    </children>
</AnchorPane>
