<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.Region?>
<?import javafx.geometry.Insets?>
<VBox fx:id="acCompetitionMenu" styleClass="sub-main-frame" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.mountCalvary.controller.basic.CompetitionMenu_controller">
   <children>
      <Region prefHeight="10.0" prefWidth="1280.0" />
      <JFXButton fx:id="btBack" onAction="#btBack_onAction" prefHeight="45.0" prefWidth="69.0" styleClass="back" text="Back">
         <graphic>
            <FontAwesomeIconView glyphName="ARROW_LEFT" size="25" />
         </graphic>
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </JFXButton>
      <VBox alignment="CENTER" prefHeight="477.0" prefWidth="1200.0" spacing="40.0" VBox.vgrow="ALWAYS">
         <children>
            <JFXButton fx:id="btCompetitionProfile" onAction="#btCompetitionProfile_onAction" prefHeight="65.0" prefWidth="554.0" styleClass="menu-button" text="Competition Profile" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btCreateCompetition" onAction="#btCreateCompetition_onAction" prefHeight="65.0" prefWidth="554.0" styleClass="menu-button" text="Create competition" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btAddEventCompetition" onAction="#btAddEventCompetition_onAction" prefHeight="65.0" prefWidth="554.0" styleClass="menu-button" text="Add event for competition" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btAddStudentCompetition" onAction="#btAddStudentCompetition_onAction" prefHeight="65.0" prefWidth="554.0" styleClass="menu-button" text="Add student for competition" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btDeleteCompetition" onAction="#btDeleteCompetition_onAction" prefHeight="65.0" prefWidth="554.0" styleClass="menu-button" text="Delete Competition" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../../css/frame.css" />
      <URL value="@../../css/button.css" />
      <URL value="@../../css/combo-box.css" />
      <URL value="@../../css/table-view.css" />
      <URL value="@../../css/text-and-password-field.css" />
      <URL value="@../../css/date-picker.css" />
      <URL value="@../../css/radio-button.css" />
      <URL value="@../../css/text-area.css" />
      <URL value="@../../css/label.css" />
   </stylesheets>
</VBox>
