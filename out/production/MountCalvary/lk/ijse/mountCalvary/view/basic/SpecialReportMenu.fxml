            <FontAwesomeIconView glyphName="ARROW_LEFT" size="25" />
         </graphic>
           <VBox.margin>
               <Insets left="10.0"/>
           </VBox.margin>
      </JFXButton>
       <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" styleClass="menu-button-pane" VBox.vgrow="ALWAYS">
         <children>
             <JFXButton fx:id="btStudentNotFollowActivity" onAction="#btStudentNotFollowActivity_onAction"
                        prefHeight="65.0" prefWidth="554.0" styleClass="menu-button"
                        text="Student who does't follow any activity" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
         </children>
      </VBox>
   </children>
    <stylesheets>
        <URL value="@../../css/frame.css"/>
