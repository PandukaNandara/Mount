<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="acUpdatePayment" prefHeight="785.0" prefWidth="1235.0"
            style="-fx-background-color: white;" styleClass="main-frame" stylesheets="@../../css/size.css"
            xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="lk.ijse.mountCalvary.controller.payment.MakePayment_controller">
    <children>
        <Label layoutX="84.0" layoutY="37.0" prefHeight="36.0" prefWidth="309.0" text="Update Payment">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <TableView fx:id="tblStudentPayment" layoutX="83.0" layoutY="227.0" prefHeight="525.0" prefWidth="1292.0">
            <columns>
                <TableColumn fx:id="colStudent_tblStudentPayment" prefWidth="336.6666564941406" text="Student"/>
                <TableColumn fx:id="colActivity_tblStudentPayment" prefWidth="187.33334350585938" text="Activity"/>
                <TableColumn fx:id="colYear_tblStudentPayment" minWidth="0.0" prefWidth="220.2398681640625"
                             text="Year"/>
                <TableColumn fx:id="colMonth_tblStudentPayment" minWidth="0.0" prefWidth="354.72314453125"
                             text="Month"/>
                <TableColumn fx:id="colFee_tblStudentPayment" prefWidth="200.08642578125" text="Fee"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <JFXComboBox fx:id="cboxActivity" labelFloat="true" layoutX="85.0" layoutY="87.0"
                     onAction="#cboxActivity_onAction" prefHeight="25.0" prefWidth="681.0" promptText="Activity"/>
        <JFXComboBox fx:id="cboxMonth" labelFloat="true" layoutX="1087.0" layoutY="88.0" onAction="#cboxMonth_onAction"
                     prefHeight="25.0" prefWidth="284.0" promptText="Month"/>
        <JFXComboBox fx:id="cboxYear" labelFloat="true" layoutX="784.0" layoutY="88.0" onAction="#cboxYear_onAction"
                     prefHeight="25.0" prefWidth="278.0" promptText="Year"/>
        <JFXTextField fx:id="txtStudentID" labelFloat="true" layoutX="85.0" layoutY="141.0"
                      onAction="#txtStudentID_onAction" prefHeight="25.0" prefWidth="142.0" promptText="Student ID"/>
        <JFXButton fx:id="btRemove_tblStudentPayment" alignment="CENTER" buttonType="RAISED" contentDisplay="TOP"
                   layoutX="1246.0" layoutY="202.0" onAction="#btRemove_tblStudentPayment_onAction" prefHeight="15.0"
                   prefWidth="128.0" text="Remove" textAlignment="CENTER" textFill="#2196f3">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btAdd" alignment="CENTER" buttonType="RAISED" contentDisplay="TOP" layoutX="1246.0"
                   layoutY="136.0" onAction="#btAdd_onAction" prefHeight="36.0" prefWidth="128.0" ripplerFill="WHITE"
                   style="-fx-background-color: #2196f3;" text="Add" textAlignment="CENTER" textFill="WHITE"/>
        <JFXButton fx:id="btSubmit" alignment="CENTER" buttonType="RAISED" contentDisplay="TOP" layoutX="1246.0"
                   layoutY="805.0" onAction="#btSubmit_onAction" prefHeight="36.0" prefWidth="128.0" ripplerFill="WHITE"
                   style="-fx-background-color: #2196f3;" text="Submit" textAlignment="CENTER" textFill="WHITE"/>
        <JFXButton fx:id="btCancel" alignment="CENTER" buttonType="RAISED" contentDisplay="TOP" layoutX="1093.0"
                   layoutY="805.0" onAction="#btCancel_onAction" prefHeight="36.0" prefWidth="128.0" text="Cancel"
                   textAlignment="CENTER" textFill="#2196f3">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </JFXButton>
        <JFXTextField fx:id="txtStudentName" labelFloat="true" layoutX="245.0" layoutY="141.0"
                      onAction="#txtStudentName_onAction" prefHeight="25.0" prefWidth="621.0"
                      promptText="Student name"/>
        <JFXTextField fx:id="txtFee" labelFloat="true" layoutX="887.0" layoutY="141.0" onAction="#txtFee_onAction"
                      prefHeight="25.0" prefWidth="322.0" promptText="Fee"/>
    </children>
</AnchorPane>
